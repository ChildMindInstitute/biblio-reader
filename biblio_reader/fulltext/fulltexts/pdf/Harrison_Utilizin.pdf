        //.........................
        // Trim a string.
        function trim(stringToTrim) {
            return stringToTrim.replace(/^\s+|\s+$/g, "");
        }
        //.........................
        // Capture the new/Changed Email Address when and if the user changes it on User Alerts popup dialog.
        function getNewEmailAddress(o) {
            emailOnPopupChanged = true;
            emailUserAlert = o.value;
        }
        //.........................
        // function that is called for User Alerts Callback
        function callBackUserAlert() {
            // For some if user changed/update value in txtUserAlertEmailAddress field, the new  value was not reflected;
            // As a result, getNewEmailAddress() function is called onblur() of txtUserAlertEmailAddress(widgetUserAlerts user control).
            // Local global variable is set to the new value in onblur().

            // get the email address.
            // if user entered a new email address or updated an existing one, get the value from the local variable.
            // else get it from the textbox itselft.
            var email;
            email = emailOnPopupChanged ? emailUserAlert : $('#txtUserAlertEmailAddress').val();

            if (emailOnPopupChanged) {
                emailOnPopupChanged = false;
            }

            var emailPattern = /\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;

            // trim the email value
            var emailTrimmed = trim(email);

            // email must have a value.  Stop!!!
            if (emailTrimmed.length == 0) {
                $(".alertPopupMessage").html('Email Address is blank. Please try again.');
                return false;
            }

            // verify if email pattern is valid.
            var regEx = new RegExp(emailPattern);
            var bMatch = regEx.test(emailTrimmed);

            // Email Address doesn't match the pattern. Invalid Email. Stop!!!
            if (bMatch == false) {
                $(".alertPopupMessage").html('Email address must be valid. Ex: name@domain.com');
                return false;
            }

            // show the progress bar...
            $('.userAlertPopupProgressBar').show();

            // initiate the callback
            callBackSaveAlert(emailTrimmed);
        }

